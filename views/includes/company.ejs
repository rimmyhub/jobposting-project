<!-- Modal -->
<div
  class="modal fade"
  id="company"
  data-bs-backdrop="static"
  data-bs-keyboard="false"
  tabindex="-1"
  aria-labelledby="staticBackdropLabel"
  aria-hidden="true"
>
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="staticBackdropLabel">
          회사 정보 수정
        </h1>
        <button
          type="button"
          class="btn-close"
          data-bs-dismiss="modal"
          aria-label="Close"
        ></button>
      </div>
      <div class="modal-body text-start">
        <!-- 회사명 -->
        <div class="modal-body">
          <div class="mb-3 row">
            <label class="col-sm-3 col-form-label">회사명</label>
            <div class="col-sm-9">
              <input class="form-control" id="userNameTag" />
            </div>
          </div>
          <!-- 회사명 -->
          <!-- 소개 -->
          <div class="mb-3 row">
            <label class="col-sm-3 col-form-label">소개</label>
            <div class="col-sm-9">
              <input class="form-control" id="userEmailTag" />
            </div>
          </div>
          <!-- 소개 -->
          <!-- 웹사이트 -->
          <div class="mb-3 row">
            <label class="col-sm-3 col-form-label">웹사이트</label>
            <div class="col-sm-9">
              <input class="form-control" id="userPhoneTag" />
            </div>
          </div>
          <!-- 웹사이트 -->
          <!-- 업계 -->
          <div class="mb-3 row">
            <label class="col-sm-3 col-form-label">업계</label>
            <div class="col-sm-9">
              <input class="form-control" id="userGenderTag" />
            </div>
          </div>
          <!-- 업계 -->
          <!-- 직원수 -->
          <div class="mb-3 row">
            <label class="col-sm-3 col-form-label">직원수</label>
            <div class="col-sm-9">
              <input class="form-control" id="userAddressTag" />
            </div>
          </div>
          <!-- 직원수 -->
          <!-- 본사주소 -->
          <div class="mb-2 row">
            <label class="col-sm-3 col-form-label">본사주소</label>
            <div class="col-sm-9">
              <input class="form-control" id="userBirthTag" />
            </div>
          </div>
          <!-- 본사주소 -->
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">
          취소
        </button>
        <button type="button" class="btn btn-primary" id="userUpdateBtn">
          저장
        </button>
      </div>
    </div>
  </div>
</div>
